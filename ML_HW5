      "source": [
        "def accuracy(y_true, y_pred):\n",
        "  accuracy = np.sum (y_true == y_pred)/ len(y_true) * 100\n",
        "  return accuracy"
      ],
      "execution_count": 44,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
